  "scripts": {
    "build": "nest build",
    "build:api": "nest build api",
    "build:worker": "nest build worker",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "start:api": "nest start api",
    "start:worker": "nest start worker",
    "start:scheduler": "nest start scheduler",
    "start:bots-telegram": "nest start bots-telegram",
    "start:bots-discord": "nest start bots-discord",
    "start:dev:api": "nest start --watch api",
    "start:dev:worker": "nest start --watch worker",
    "start:dev:scheduler": "nest start --watch scheduler",
    "start:dev:bots-telegram": "nest start --watch bots-telegram",
    "start:dev:bots-discord": "nest start --watch bots-discord",
    "typeorm:api": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d apps/api/src/config/database/typeorm-nest/ormconfig.ts",
    "dbm:api:generate": "npm run typeorm:api -- migration:generate apps/api/src/infrastructure/database/typeorm-nest/migrations/$npm_config_name",
    "dbm:api:drop": "npm run typeorm:api -- schema:drop",
    "dbm:api:run": "npm run typeorm:api -- migration:run",
    "dbm:api:revert": "npm run typeorm:api -- migration:revert"
  },
